{
  "manifest_version": 2,
  "name": "URL Blocker",
  "version": "0.0.0.1",
  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["script/script.js"]
  }
}
